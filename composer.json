{
    "name":"digitalkaoz.net",
    "type":"webpage",
    "target-dir":"vendor/",
    "homepage":"http://digitalkaoz.net",

    "require":{
        "symfony/symfony":">=2.1-dev,<2.2",
        "symfony/assetic-bundle":"master",
        "symfony/security-bundle":">=2.1-dev,<2.2",
        "symfony/swiftmailer-bundle":"master",
        "symfony/monolog-bundle":"master",
        "symfony/twig-bundle":"*",

        "twig/extensions":"*",
        "sensio/generator-bundle":">2.0",
        "sensio/framework-extra-bundle":">2.0",
        "sensio/distribution-bundle":"*",

        "doctrine/doctrine-bundle":"master",
        "doctrine/orm":"2.2.*",
        "doctrine/doctrine-fixtures-bundle":">=2.1-dev,<2.2",

        "jms/security-extra-bundle":">=1.1.0",
        "jms/aop-bundle":"1.0.0",

        "sonata-project/doctrine-orm-admin-bundle":"*",
        "sonata-project/user-bundle":"*",

        "behat/mink-bundle":"master",
        "behat/behat-bundle":">=1.0",

        "knplabs/knp-menu-bundle":"*",
        "knplabs/knp-paginator-bundle":"*",
        "knplabs/knp-last-tweets-bundle":"*",
        "knplabs/knp-zend-cache-bundle":"*",
        "knplabs/knp-disqus-bundle":"*",

        "ruflin/Elastica":"*",
        "kriswallsmith/buzz":">=0.5",
        "friendsofsymfony/user-bundle":"1.1.0",
        "mopa/bootstrap-bundle":"master",
        "stof/doctrine-extensions-bundle":"*",
        "vich/uploader-bundle":"master",
        "exercise/elastica-bundle":"*",
        "elao/profiler-extra-bundle":"2.0",

        "ornicar/github-api":"*",
        "ornicar/akismet-bundle":"dev-master",

        "twitter/bootstrap":">=2.0",
        "mrdoob/threejs":">=0.48",
        "elastic/search":"0.19.1",
        "monofone/connect-bundle":"dev-composer_fix",
        "ihqs/contact-bundle":"dev-composer"
    },

    "autoload":{
        "psr-0":{
            "rs":"src/"
        }
    },

    "repositories":[
        {
            "type":"vcs",
            "url":"git://digitalkaoz@github.com/digitalkaoz/BlageConnectBundle"
        },
        {
            "type":"vcs",
            "url":"git://digitalkaoz@github.com/digitalkaoz/ContactBundle.git"
        },
        {
            "type":"package",
            "package":{
                "name":"elastic/search",
                "description":"elasticsearch",
                "version":"0.19.1",
                "dist":{
                    "url":"https://github.com/downloads/elasticsearch/elasticsearch/elasticsearch-0.19.1.zip",
                    "type":"zip"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"twitter/bootstrap",
                "description":"twitter bootstrap",
                "version":"2.0.2",
                "dist":{
                    "url":"http://twitter.github.com/bootstrap/assets/bootstrap.zip",
                    "type":"zip"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"mrdoob/threejs",
                "description":"three js",
                "version":"0.48.0",
                "dist":{
                    "url":"https://github.com/mrdoob/three.js/zipball/r48",
                    "type":"zip"
                }
            }
        },
        {
            "type":"package",
            "package":{
                "name":"zendframework/zend-cache",
                "version":"2.0.0beta1",
                "dist":{
                    "url":"http://github.com/KnpLabs/zend-cache/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"git://github.com/KnpLabs/zend-cache.git",
                    "type":"git",
                    "reference":"master"
                },
                "autoload":{
                    "psr-0":{
                        "Zend\\Cache":""
                    }
                },
                "target-dir":"Zend/Cache"
            }
        },
        {
            "type":"package",
            "package":{
                "name":"elao/profiler-extra-bundle",
                "version":"2.0",
                "dist":{
                    "url":"https://github.com/Elao/WebProfilerExtraBundle/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"git://github.com/Elao/WebProfilerExtraBundle.git",
                    "type":"git",
                    "reference":"master"
                },
                "autoload":{
                    "psr-0":{
                        "Elao\\WebProfilerExtraBundle" : ""
                    }
                },
                "target-dir":"Elao/WebProfilerExtraBundle"
            }
        },
        {
            "type":"package",
            "package":{
                "name":"zendframework/zend-filter",
                "version":"2.0.0beta1",
                "dist":{
                    "url":"http://github.com/KnpLabs/zend-filter/zipball/master",
                    "type":"zip"
                },
                "source":{
                    "url":"git://github.com/KnpLabs/zend-filter.git",
                    "type":"git",
                    "reference":"master"
                },
                "autoload":{
                    "psr-0":{
                        "Zend\\Filter":""
                    }
                },
                "target-dir":"Zend/Filter"
            }
        }
    ]
}

