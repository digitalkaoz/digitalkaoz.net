{
    "name" : "digitalkaoz.net",
    "type" : "webpage",
    "target-dir" : "vendor/",
    "homepage" : "http://digitalkaoz.net",    

    "require": {
        "php":                                  ">=5.3.2",
        "symfony/symfony":                      ">=2.0.9,<2.1.0-dev",
        "symfony/assetic-bundle":               "*",
        "symfony/security-bundle":              ">=2.0.9,<2.1.0-dev",
        "doctrine/orm":                         "2.1.*",
        "doctrine/data-fixtures":               "*",
        "doctrine/doctrine-fixtures-bundle":    ">=2.0.1,<2.1.0-dev",
        "twig/extensions":                      "*",
        "sensio/generator-bundle":              "2.0.*",
        "sensio/framework-extra-bundle":        "2.0.*",
        "sensio/distribution-bundle":           "2.0.*",
        "jms/security-extra-bundle":            ">=1.1.0",
        "jms/aop-bundle":                       "1.0.0",
        "sonata/doctrine-orm-admin-bundle":     "*",
        "sonata/user-bundle":                   "*",
        "behat/mink-bundle":                    "*",
        "behat/mink":                           "*",
        "behat/behat-bundle":                   "*",
        "knplabs/knp-menu-bundle":              "*",
        "knplabs/knp-paginator-bundle":         "*",
        "knplabs/knp-last-tweets-bundle":       "*",
        "ruflin/Elastica":                      "*",
        "ornicar/github-api":                   "*",
        "kriswallsmith/buzz":                   "*",
        "friendsofsymfony/user-bundle":         "1.1.0",
        "mopa/bootstrap-bundle":                "*",
        "stof/doctrine-extensions-bundle":      "*",

        "ornicar/akismet-bundle":               "*",
        "knplabs/knp-zend-cache-bundle":        "*",
        "knplabs/knp-disqus-bundle":            "*",
        "ihqs/contact-bundle":                  "*",
        "monofone/connect-bundle":              "*",
        "foq/elastica-bundle":                  "*",
        "twitter/bootstrap":                    "*",
        "elao/profile-extra-bundle":            "*",
        "elastic/search":                       "*"

    },

    "repositories" : {
        "knplabs-disqus-bundle": {
            "package": {
                "name": "knplabs/knp-disqus-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/KnpDisqusBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/KnpDisqusBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "zendframework/zend-cache": "*",
                    "zendframework/zend-filter": "*"
                },
                "autoload": {
                    "psr-0": { "Knp\\Bundle\\DisqusBundle": "" }
                },
                "target-dir": "Knp/Bundle/DisqusBundle"
            }
        },
        "knp-zend-cache-bundle": {
            "package": {
                "name": "knplabs/knp-zend-cache-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/KnpZendCacheBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/KnpZendCacheBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "require": {
                    "zendframework/zend-cache": "*",
                    "zendframework/zend-filter": "*"
                },
                "autoload": {
                    "psr-0": { "Knp\\Bundle\\ZendCacheBundle": "" }
                },
                "target-dir": "Knp/Bundle/ZendCacheBundle"
            }
        },
        "zendframework-zend-cache": {
            "package": {
                "name": "zendframework/zend-cache",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/zend-cache/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-cache.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Zend\\Cache": "" }
                },
                "target-dir": "Zend/Cache"
            }
        },
        "zendframework-zend-filter": {
            "package": {
                "name": "zendframework/zend-filter",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/KnpLabs/zend-filter/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/KnpLabs/zend-filter.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Zend\\Filter": "" }
                },
                "target-dir": "Zend/Filter"
            }
        },
        "ornicar-aksimet-bundle": {
            "package": {
                "name": "ornicar/akismet-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/ornicar/OrnicarAkismetBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/ornicar/OrnicarAkismetBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Ornicar\\AkismetBundle": "" }
                },
                "target-dir": "Ornicar/AkismetBundle"
            }
        },
        "ihqs-contact-bundle": {
            "package": {
                "name": "ihqs/contact-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/ihqs/ContactBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/ihqs/ContactBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "IHQS\\ContactBundle": "" }
                },
                "target-dir": "IHQS/ContactBundle"
            }
        },
        "blage-connect-bundle": {
            "vcs": {
                "url": "git://github.com/digitalkaoz/BlageConnectBundle.git"
            }
        },
        "foq-elastica-bundle": {
            "package": {
                "name": "foq/elastica-bundle",
                "version": "2.0",
                "dist": {
                    "url": "https://github.com/Exercise/FOQElasticaBundle/zipball/2.0",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/Exercise/FOQElasticaBundle.git",
                    "type": "git",
                    "reference": "branches/2.0"
                },
                "autoload": {
                    "psr-0": { "FOQ\\ElasticaBundle": "" }
                },
                "target-dir": "FOQ/ElasticaBundle"
            }
        },
        "elao-profiler-bundle": {
            "package": {
                "name": "elao/profile-extra-bundle",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/Elao/WebProfilerExtraBundle/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/Elao/WebProfilerExtraBundle.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": { "Elao\\WebProfilerExtraBundle": "" }
                },
                "target-dir": "Elao/WebProfilerExtraBundle"
            }
        },
        "twitter-bootstrap": {
            "package": {
                "name": "twitter/bootstrap",
                "version": "master-dev",
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "git://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        "elastic-search": {
            "package": {
                "name": "elastic/search",
                "version": "0.18.7",
                "dist": {
                    "url": "https://github.com/downloads/elasticsearch/elasticsearch/elasticsearch-0.18.7.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/elasticsearch/elasticsearch.git",
                    "type": "git",
                    "reference": "master"
                },
                "bin": ["bin/elasticsearch"]
            }
        }
    },

    "autoload": {
        "psr-0": {
            "rs": "src/"
        }
    }
}

